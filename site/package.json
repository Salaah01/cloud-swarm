    "axios": "^0.21.4",
    "cairo": "0.1.0-alpha.3",
    "caniuse-lite": "^1.0.30001207",
    "chart.js": "^2.9.4",
    "concurrently": "^6.0.0",
    "csv": "^5.4.0",
    "deepmerge": "^4.2.2",
    "django-react-csrftoken": "^1.0.4",
    "file-loader": "^6.0.0",
    "radium": "^0.26.1",
    "random-flat-colors": "^1.0.7",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-chartjs-2": "^2.11.1",
    "react-color": "^2.18.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.3.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "workbox-cacheable-response": "^6.1.2",
    "workbox-expiration": "^6.1.2",
    "workbox-routing": "^6.1.2",
    "workbox-strategies": "^6.1.2"
  },
  "browsers": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
